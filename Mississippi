Please_enter_a_string = "Mississippi"

frequency_table = {} 
  
for char in Please_enter_a_string: 
    frequency_table[char] = frequency_table.get(char, 0) + 1

print ("Character frequency table for '{}' is :\n {}".format(Please_enter_a_string, str(frequency_table)))

